#!/usr/bin/env python

from pyfzf import FzfPrompt
from lib.FirefoxProfile import FirefoxProfile


if __name__ == '__main__':
    ff = FirefoxProfile.default()
    choice = FzfPrompt().prompt(ff.history())
    if choice:
        ff.open("http" + choice[0].split(", http")[1])
    else:
        print("Aborted")
